{
  "name": "blog",
  "version": "0.2.0",
  "description": "NeonWilderness Blog",
  "private": true,
  "main": "app.js",
  "scripts": {
    "cp:analyze": "node ./cockpit/import --convert --analyze --stop",
    "cp:convert": "node ./cockpit/import --convert --stop",
    "cp:import": "node ./cockpit/import --convert --xlimit=40 --xoldest",
    "cp:initialize": "node ./cockpit/initialize",
    "dev": "nuxt",
    "dev:debug": "node --inspect node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "npm run generate && npm run sync",
    "sync": "node ./cockpit/sync",
    "test": "node ./cockpit/test",
    "tapi": "node ./cockpit/testApi",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/toast": "^3.0.1",
    "ellipsize": "^0.1.0",
    "escape-goat": "^1.3.0",
    "innertext": "^1.0.3",
    "load-scripts": "^1.0.0",
    "lodash.merge": "^4.6.1",
    "md5": "^2.2.1",
    "nuxt": "^1.4.4",
    "v-runtime-template": "^1.5.2",
    "vue": "^2.5.17",
    "vue-component-component": "0.0.1",
    "vue-timeago": "^5.0.0",
    "vuetify": "^1.3.3"
  },
  "devDependencies": {
    "@nuxtjs/feed": "^0.1.1",
    "@nuxtjs/sitemap": "^0.1.1",
    "@types/node": "^10.11.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cockpit-sdk": "^0.9.4",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.7.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.7.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "pretty": "^2.0.0",
    "yargs": "^12.0.2"
  }
}
