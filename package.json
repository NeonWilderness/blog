{
  "name": "blog",
  "version": "0.1.0",
  "description": "NeonWilderness Blog",
  "main": "app.js",
  "scripts": {
    "cp:analyze": "node ./cockpit/import --convert --analyze --stop",
    "cp:convert": "node ./cockpit/import --convert --stop",
    "cp:import": "node ./cockpit/import --limit=40 --oldest",
    "cp:initialize": "node ./cockpit/initialize",
    "json": "node ./cockpit/jsonbase",
    "dev": "nuxt",
    "dev:debug": "node --inspect node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "npm run generate && npm run sync",
    "sync": "node ./cockpit/sync",
    "test": "node ./cockpit/test",
    "tapi": "node ./cockpit/testApi",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/dotenv": "^1.1.1",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/toast": "^3.0.1",
    "@nuxtjs/vuetify": "^0.4.2",
    "cockpit-api-client": "^1.0.4",
    "ellipsize": "^0.1.0",
    "escape-goat": "^1.3.0",
    "lodash.merge": "^4.6.1",
    "nuxt": "^1.4.2",
    "vue-timeago": "^5.0.0"
  },
  "devDependencies": {
    "@nuxtjs/feed": "^0.1.1",
    "@nuxtjs/sitemap": "^0.1.1",
    "@types/node": "^10.5.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cockpit-sdk": "^0.9.4",
    "dotenv-safe": "^6.0.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.7.1",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "yargs": "^12.0.1"
  }
}
