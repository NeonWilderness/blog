{
  "name": "blog",
  "version": "0.4.0",
  "description": "NeonWilderness Blog",
  "private": true,
  "main": "app.js",
  "scripts": {
    "cp:analyze": "node ./cockpit/import --convert --analyze --stop",
    "cp:convert": "node ./cockpit/import --convert --stop",
    "cp:import": "node ./cockpit/import --convert --xlimit=40 --xoldest",
    "cp:initialize": "node ./cockpit/initialize",
    "dev": "nuxt",
    "dev:debug": "node --inspect node_modules/nuxt/bin/nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "SET NODE_OPTIONS='--max-old-space-size=4096' && nuxt generate",
    "deploy": "npm run generate && npm run sync",
    "sync": "node ./cockpit/sync",
    "test": "node ./cockpit/test",
    "tapi": "node ./cockpit/testApi",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {
    "@nuxt/core": "^2.4.5",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/toast": "^3.0.2",
    "date-fns": "^1.30.1",
    "ellipsize": "^0.1.0",
    "escape-goat": "^1.3.0",
    "innertext": "^1.0.3",
    "load-scripts": "^1.0.0",
    "lodash.merge": "^4.6.1",
    "md5": "^2.2.1",
    "nuxt": "^2.4.5",
    "nuxt-svg-loader": "^1.0.1",
    "vue": "^2.6.7",
    "vue-component-component": "0.0.1",
    "vue-timeago": "^5.0.0",
    "vuetify": "^1.5.2"
  },
  "devDependencies": {
    "@nuxtjs/feed": "^1.1.0",
    "@nuxtjs/sitemap": "^0.2.0",
    "@types/node": "^10.12.27",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "cockpit-sdk": "^0.15.1",
    "cp-file": "^6.0.0",
    "dotenv-safe": "^6.1.0",
    "eslint": "^5.14.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "glob": "^7.1.3",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "pretty": "^2.0.0",
    "yargs": "^13.2.1"
  }
}
